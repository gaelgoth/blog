{
  "name": "gael-blog",
  "type": "module",
  "version": "0.0.1",
  "license": "WTFPL",
  "repository": "importantimport/urara",
  "homepage": "https://github.com/importantimport/urara",
  "bugs": "https://github.com/importantimport/urara/issues",
  "author": "Ëóç+85CD",
  "packageManager": "pnpm@9.1.1",
  "scripts": {
    "clean": "node urara.js clean",
    "tsc": "tsc -p tsconfig.node.json",
    "tsc:watch": "tsc -w -p tsconfig.node.json",
    "urara:build": "node urara.js build",
    "urara:watch": "node urara.js watch",
    "kit:dev": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite dev",
    "kit:build": "cross-env NODE_OPTIONS=--max_old_space_size=7680 vite build",
    "dev:parallel": "run-p -r tsc:watch urara:watch \"kit:dev {@} \" --",
    "dev": "run-s tsc \"dev:parallel {@} \" --",
    "build": "npm-run-all -s tsc urara:build kit:build clean",
    "preview": "vite preview",
    "start": "cross-env ADAPTER=node pnpm build && node build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@iconify-json/heroicons-outline": "^1.2.1",
    "@iconify-json/heroicons-solid": "^1.2.1",
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-netlify": "^4.4.1",
    "@sveltejs/adapter-node": "^5.2.12",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/adapter-vercel": "^5.6.3",
    "@sveltejs/kit": "^2.16.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.2",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.13.5",
    "@types/unist": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@unocss/extractor-svelte": "^66.0.0",
    "@vite-pwa/sveltekit": "^0.1.3",
    "chalk": "^5.4.1",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "daisyui": "^4.12.23",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.1",
    "fenceparser": "^2.2.0",
    "fff-flavored-frontmatter": "1.2.1",
    "github-slugger": "^2.0.0",
    "mdast-util-to-string": "^3.2.0",
    "mdsvex": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-lightningcss": "^1.0.1",
    "prettier": "^3.5.2",
    "prettier-plugin-svelte": "^3.3.3",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-slug": "^6.0.0",
    "remark": "^15.0.1",
    "remark-fff": "1.2.1",
    "remark-gfm": "^4.0.1",
    "shiki-twoslash": "^3.1.2",
    "svelte": "^4.2.19",
    "svelte-check": "^3.8.6",
    "svelte-eslint-parser": "^0.43.0",
    "svelte-preprocess": "^6.0.3",
    "sveltekit-embed": "^0.0.14",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "unist-util-visit": "^4.1.2",
    "unocss": "^66.0.0",
    "vite": "^5.4.14",
    "vite-imagetools": "^4.0.19",
    "vite-plugin-pwa": "^0.21.1",
    "workbox-build": "^7.3.0",
    "workbox-window": "^7.3.0"
